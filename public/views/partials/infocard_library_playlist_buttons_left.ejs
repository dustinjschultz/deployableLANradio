
<div class="edit-tags-song">
    <button onclick="createNewTag(this)" class="hidden" title="New tag">
        <i class="fa fa-plus-circle fa-lg" style="width: 20px; height: 20px;"></i>
    </button>
    <button onclick="togglePlaylistEdits(this)">
        <i class="fa fa-pencil fa-lg" style="width: 20px; height: 20px;"></i>
    </button>
    <button class="hidden" onclick="savePlaylist(this)">
        <i class="fa fa-save fa-lg" style="width: 20px; height: 20px;"></i> 
    </button>
</div>
